<?xml version="1.0" encoding="utf-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://maven.apache.org/POM/4.0.0"
           xmlns="http://maven.apache.org/POM/4.0.0"  elementFormDefault="qualified">
    <xs:element name="configuration">
        <xs:complexType>
            <xs:all>
                <xs:element name="title" type="xs:string" default="应用配置文件" minOccurs="0"/>
                <xs:element name="property" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="key"/>
                            <xs:element name="value"/>
                            <xs:element name="type"/>
                            <xs:element name="description"  minOccurs="0"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:all>
        </xs:complexType>
    </xs:element>

</xs:schema>